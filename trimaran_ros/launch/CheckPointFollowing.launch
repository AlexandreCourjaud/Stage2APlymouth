<launch>


<node pkg="rosserial_arduino" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="115200"/>
  </node>

<param name="mode" type="int" value="0" />
<param name="file" type="string" value="$(arg file)" />


<node pkg="trimaran_ros" type="mode" name="mode" />
<node pkg="trimaran_ros" type="gps.py" name="gps" />
<node pkg="WRSC_plymouth_JEGAT" type="filter_wind.py" name="filter_wind"/>
<node pkg="trimaran_ros" type="lineFollow" name="lineFollow"/>
<node pkg="trimaran_ros" type="checkPointFollow.py" name="checkPointFollow" />
<node name="record" pkg="rosbag" type="record" args="record -a -o $(find trimaran_ros)/rosbag/checkPointFollow" />


</launch>
